{
  "name": "PreSonus StudioLive Control",
  "version": "1.0.0",
  "slug": "presonus-studiolive",
  "description": "Control PreSonus StudioLive III mixers from Home Assistant",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {
    "8099/tcp": 8099
  },
  "ports_description": {
    "8099/tcp": "Web interface and API"
  },
  "options": {
    "mixer_ip": "192.168.1.100",
    "mixer_port": 53000,
    "log_level": "info"
  },
  "schema": {
    "mixer_ip": "str",
    "mixer_port": "int(1,65535)?",
    "log_level": "list(debug|info|warn|error)?"
  },
  "image": "ghcr.io/joellogan/presonus-studiolive-{arch}"
}
